@model IEnumerable<StartGrow.Models.Proyecto>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Nombre)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.MinInversion)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Plazo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Interes)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Importe)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Progreso)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.NumInversores)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FechaExpiracion)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.Areas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rating)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MinInversion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Plazo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Interes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Importe)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Progreso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumInversores)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaExpiracion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Areas.TiposInversionesID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rating.RatingID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ProyectoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ProyectoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ProyectoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
